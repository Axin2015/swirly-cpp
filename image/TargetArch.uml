@startuml

interface "ZeroMQ" as ZMQ
interface HTTP

package "Doobry Daemon" {
  [dbr_serv] <<library>>
  database SQLite
}
dbr_serv - ZMQ

package "Doobry Shell" {
  [dbr_clnt] <<library>>
}
ZMQ <.. [dbr_clnt]

package "LigHTTPd" {
  [dbr_mod] <<module>>
}
ZMQ <.. [dbr_mod]

dbr_mod - HTTP

cloud Internet {
  [Web 2.0 Client]
}
HTTP <.. Internet

@enduml
