@startuml

interface "ZeroMQ" as ZMQ
interface HTTP

package "Doobry Daemon" {
  [dbr_serv] <<library>>
  database SQLite
}
dbr_serv - ZMQ

package "Desktop Client" {
  [dbr_clnt] <<library>>
}
ZMQ <.. [dbr_clnt]

package "Web Server" {
  [dbrpy] <<module>>
}
ZMQ <.. [dbrpy]

dbrpy - HTTP

cloud Internet {
  [Web 2.0 Client]
}
HTTP <.. Internet

@enduml
